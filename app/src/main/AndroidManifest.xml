<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hw.cy.app">

    <!--访问网络权限-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--读写SD卡权限-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".CheYouApplication"
        android:allowBackup="true"
        android:icon="@mipmap/cy"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.MainActivity">

        </activity>

        <activity android:name=".view.activity.LoginActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--购物车-->
        <activity android:name=".view.activity.ShoppingCartActivity" />
        <!--洗车-->
        <activity android:name=".view.activity.CarWashStoreActivity" />
        <!--商店详情-->
        <activity android:name=".view.activity.CarStoreDetailActivity" />
        <!--汽车超市-->
        <activity android:name=".view.activity.CarShopMarketActivity" />
        <!--商品列表-->
        <activity android:name=".view.activity.CarGoodsActivity"/>
        <!--汽车资讯  webview-->
        <activity android:name=".view.activity.ActivityWebView" />
        <!--个人资料-->
        <activity android:name=".view.activity.ProfileActivity" />
        <!--商品收藏-->
        <activity android:name=".view.activity.CollectActivity" />
        <!--商品详情-->
        <activity android:name=".view.activity.CarGoodsDetailActivity"/>
        <!--我的订单-->
        <activity android:name=".view.activity.UserOrderActivity"/>
    </application>

</manifest>